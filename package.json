{
  "version": "0.1.242",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "tsc-check": "./node_modules/typescript/bin/tsc -p ./tsconfig.json"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run tsc-check"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ga-4-react",
  "author": "manueltrebbi",
  "repository": "https://github.com/unrealmanu/ga-4-react",
  "module": "dist/ga-4-react.esm.js",
  "size-limit": [
    {
      "path": "dist/ga-4-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ga-4-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.7.0",
    "@testing-library/react": "^11.1.2",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "husky": "^4.3.0",
    "jsdom": "^16.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "react",
    "google",
    "analytics",
    "ga"
  ]
}
